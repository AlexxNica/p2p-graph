{
  "name": "p2p-graph",
  "description": "Real-time P2P network visualization with D3",
  "version": "1.0.2",
  "author": {
    "name": "Feross Aboukhadijeh",
    "email": "feross@feross.org",
    "url": "http://feross.org/"
  },
  "bugs": {
    "url": "https://github.com/feross/p2p-graph/issues"
  },
  "dependencies": {
    "d3": "3.5.6",
    "debounce": "1.0.0",
    "debug": "2.2.0",
    "prettier-bytes": "1.0.3"
  },
  "devDependencies": {
    "async": "2.0.1",
    "browserify": "14.1.0",
    "http-server": "0.9.0",
    "standard": "*"
  },
  "homepage": "https://github.com/feross/p2p-graph",
  "keywords": [
    "real-time",
    "p2p",
    "peer-to-peer",
    "network",
    "visualization",
    "d3",
    "graph"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/feross/p2p-graph.git"
  },
  "scripts": {
    "test": "standard",
    "start": "npm run bundle && npm run example && npm run server",
    "bundle": "browserify -r ./index.js:p2p-graph > example/dist/p2p-graph.bundle.js",
    "example": "browserify -r async -r debug > example/libs.js",
    "server": "http-server example/ -p 2222"
  },
  "standard": {
    "ignore": [
      "example/"
    ]
  }
}
